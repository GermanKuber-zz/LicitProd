<Window x:Class="LicitProd.UI.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LicitProd.UI.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="166.5" Width="327.5">
    <StackPanel>
        <PasswordBox x:Name="passwordBox" Margin="20,20,20,5"/>
        <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="20,10,20,0"/>
        <StackPanel  Orientation="Horizontal" HorizontalAlignment="Right">
            <Button x:Name="btnLogin" Click="BtnLogin_Click"  Content="Loguear" Width="90" d:LayoutOverrides="Height" Margin="20,8,20,0"/>
            <Button x:Name="btnCancelar" Click="BtnCancelar_Click" Content="Cancelar" Width="90" d:LayoutOverrides="Height" Margin="0,8,20,0"/>
        </StackPanel>
    </StackPanel>
</Window>
